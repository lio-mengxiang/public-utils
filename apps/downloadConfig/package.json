{
  "name": "@mx-design/download-config",
  "version": "0.0.1",
  "description": "download config file about eslint、prettier",
  "bin": {
    "mx-download": "./bin/index.js"
  },
  "engines": {
    "node": ">=18.2"
  },
  "scripts": {
    "b": "rimraf ./esm && rollup -c",
    "clean": "rimraf {lib,types} --glob",
    "build": "pnpm clean && tsc --build --force tsconfig.build.json",
    "dev": "tsc --build tsconfig.build.json -w",
    "clean-nm": "rimraf node_modules"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "12.1.0",
    "compressing": "^1.10.1",
    "content-disposition": "^0.5.4",
    "ext-name": "^5.0.0",
    "inquirer": "^12.0.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "4.24.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-node-externals": "^7.1.3",
    "typescript": "5.6.2"
  }
}